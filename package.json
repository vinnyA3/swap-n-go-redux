    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^3.1.0",
    "expect": "^1.20.2",
    "jest": "^20.0.1",
    "jsdom": "^10.1.0",
    "mocha": "^3.4.1",
    "npm-run-all": "^4.0.2",
    "open": "^0.0.5",
    "react-addons-test-utils": "^15.5.1",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
